  "main": "packages/inferno/dist/inferno.js",
  "jsnext:main": "packages/inferno/dist/inferno.es2015.js",
  "keywords": [
    "babel",
    "react",
    "inferno",
    "framework",
    "interfaces",
    "user interfaces",
    "html",
    "renderToString",
    "server",
    "dom",
    "browser",
    "rollup"
  ],
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "typings"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "html",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/trueadm/inferno.git"
  },
  "scripts": {
    "browser": "webpack-dev-server --config config/webpack.dev.conf.js",
    "build": "npm run clean && mkdirp dist && npm run build:dev && npm run build:prod",
    "build:dev": "cross-env NODE_ENV=development babel-node --presets es2015 config/rollup.config.js",
    "build:prod": "cross-env NODE_ENV=production babel-node --presets es2015 config/rollup.config.js",
    "clean": "rimraf coverage/ dist/",
    "eslint:source": "eslint ./src/**/*.js",
    "eslint:test": "eslint ./src/**/__tests__/*.js",
    "karma:firefox": "karma start config/karma.conf.js --browsers=Firefox",
    "karma:chrome": "karma start config/karma.conf.js --browsers=Chrome",
    "karma:ie": "karma start config/karma.conf.js --browsers=IE",
    "karma:all": "npm run karma:firefox && npm run karma:chrome && npm run karma:ie",
    "lint:source": "tslint 'src/**/*.ts'",
    "packages:purge": "rimraf node_modules",
    "packages:reinstall": "npm run packages:purge && npm install",
    "prepublish": "npm run build",
    "prepush": "npm run eslint:source && npm lint:source && npm test",
    "server:cov": "cross-env nyc mocha --opts config/mocha.opts",
    "server:cov:publish": "npm run server:cov && coveralls < coverage/lcov.info && rimraf coverage",
    "test": "npm run test:browser && npm run test:server",
    "test:browser": "karma start config/karma.conf.js",
    "test:server": "cross-env mocha --opts config/mocha.opts"
  },
  "pre-commit": [
    "eslint:source",
    "lint:source",
    "test"
  ],
  "dependencies": {}
}
