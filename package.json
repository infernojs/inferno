  ],
  "homepage": "https://github.com/trueadm/inferno#readme",
  "main": "packages/inferno/dist/inferno.js",
  "jsnext:main": "packages/inferno/dist/inferno.es2015.js",
  "keywords": [
    "babel",
    "react",
    "inferno",
    "framework",
    "interfaces",
    "user interfaces",
    "html",
    "renderToString",
    "server",
    "dom",
    "browser",
    "rollup"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/trueadm/inferno.git"
  },
  "scripts": {
    "clean": "rimraf coverage/ dist/",
    "build": "npm run clean && mkdirp dist && npm run build:dev && npm run build:prod",
    "build:dev": "cross-env NODE_ENV=development babel-node --presets es2015 config/rollup.config.js",
    "build:prod": "cross-env NODE_ENV=production babel-node --presets es2015 config/rollup.config.js",
    "test": "npm run test:browser",
    "test:browser": "./node_modules/.bin/karma start config/karma.conf.js --no-auto-watch --single-run",
    "watch:browser": "./node_modules/.bin/karma start config/karma.conf.js --auto-watch",
    "watch:chrome": "./node_modules/.bin/karma start config/karma.conf.js --auto-watch  --browsers=Chrome",
    "test:server": "mocha --recursive --bail --opts config/mocha.opts",
    "watch:server": "npm run test-server -- --watch",
    "server:cov": "babel-node node_modules/isparta/bin/isparta cover node_modules/mocha/bin/_mocha --report lcovonly -- -R spec --recursive src/**/*__tests__*/ --opts config/mocha.opts",
    "server:cov:publish": "npm run server:cov && coveralls < coverage/lcov.info && rimraf coverage",
    "eslint:source": "eslint ./src/**/*.js",
    "eslint:test": "eslint ./src/**/__tests__/*.js",
    "karma:firefox": "./node_modules/.bin/karma start config/karma.conf.js --browsers=Firefox --single-run",
    "karma:chrome": "./node_modules/.bin/karma start config/karma.conf.js --browsers=Chrome",
    "karma:ie": "./node_modules/.bin/karma start config/karma.conf.js --browsers=IE --single-run",
    "karma:all": "npm run karma:firefox && npm run karma:chrome && npm run karma:ie",
    "packages": "npm list --depth=0",
    "packages:purge": "rm -rf node_modules",
    "packages:reinstall": "npm run packages:purge && npm install",
    "packages:check": "ncu",
    "packages:upgrade": "ncu -u",
    "browser": "webpack-dev-server --quiet --config config/webpack.dev.conf.js"
  },
  "pre-commit": [
    "eslint:source",
    "test"
  ]
}
