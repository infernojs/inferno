  "version": "1.1.2",
  "author": {
    "name": "Dominic Gannaway",
    "email": "dg@domgan.com"
  },
  "bugs": {
    "url": "https://github.com/infernojs/inferno/issues"
  },
  "devDependencies": {
    "@types/benchmark": "^1.0.30",
    "@types/chai": "^3.4.34",
    "@types/core-js": "^0.9.35",
    "@types/history": "^2.0.44",
    "@types/mocha": "^2.2.37",
    "@types/node": "^7.0.0",
    "@types/sinon": "^1.16.34",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-inferno": "^1.5.0",
    "babel-plugin-module-resolver": "^2.4.0",
